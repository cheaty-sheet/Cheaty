{
  "name": "@cheaty-sheet/cheaty",
  "version": "0.8.0",
  "description": "Cheat Sheet generation made easy",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "> 8.0.0",
    "npm": "> 5.0.0"
  },
  "scripts": {
    "build": "tsc && npm run resources",
    "resources": "cp -R resources/ dist/resources/ && cp -R test/resources/ dist/test/resources/",
    "docs": "typedoc --options ./typedoc.json ./src/lib",
    "lint": "eslint src/ --ext .js,.ts",
    "test": "npx jest",
    "clean": "rm -rf dist"
  },
  "bin": "./dist/src/cli.js",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/debug": "^4.1.2",
    "@types/mkdirp": "^0.5.2",
    "debug": "^4.1.1",
    "handlebars": "^4.1.2",
    "js-yaml": "^3.13.1",
    "marked": "^0.7.0",
    "mkdirp": "^0.5.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/marked": "^0.6.1",
    "@types/node": "^10.12.21",
    "@types/yargs": "^12.0.9",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "eslint": "^5.13.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  }
}
