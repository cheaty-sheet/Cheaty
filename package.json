{
  "name": "@cheaty-sheet/cheaty",
  "version": "0.4.0",
  "description": "Cheat Sheet generation made easy",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "> 8.0.0",
    "npm": "> 5.0.0"
  },
  "scripts": {
    "build": "tsc && npm run resources",
    "resources": "cp -R resources/ dist/resources/ && cp -R test/resources/ dist/test/resources/",
    "docs": "typedoc --options ./typedoc.json ./src/lib",
    "lint": "eslint src/ --ext .js,.ts",
    "test": "npm run build && mocha dist/test",
    "clean": "rm -rf dist"
  },
  "bin": "./dist/src/cli.js",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "handlebars": "^4.1.0",
    "js-yaml": "^3.12.1",
    "marked": "^0.6.1",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/yargs": "^12.0.9",
    "@types/chai": "^4.1.7",
    "@types/marked": "^0.6.1",
    "@types/mocha": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "mocha": "^6.0.0",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.1"
  }
}
